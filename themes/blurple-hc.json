{
  "name": "Charc Blurple SIDDH HC",
  "type": "dark",
  "colors": {
        
        "editor.background": "#222222",
        "editor.foreground": "#5F65B5",
        "panel.background": "#222222",
        "terminal.background": "#181818",
        "sideBar.background": "#1e1e1e",
        "activityBar.background": "#1d1d1d",
        "auxiliaryBar.background": "#181818",
        "editorWidget.background": "#181818",
        "quickInput.background": "#181818",
        "input.background": "#151515",
        "input.foreground": "#ffffff",
        "input.border": "#333333",

        
        "foreground": "#7176b5",
        "activityBar.foreground": "#7176b5",
        "panelTitle.activeForeground": "#7176b5",
        "sideBarSectionHeader.foreground": "#7176b5",
        "breadcrumb.foreground": "#7176b5",
        "list.highlightForeground": "#7176b5",
        "textLink.activeForeground": "#7176b5",
        "list.activeSelectionForeground": "#ffffff",
        
        
        "focusBorder": "#767b91",
        "widget.border": "#767b91",
        "sideBar.border": "#333333",
        "panel.border": "#333333",
        "activityBar.border": "#1e1e1e",
        
        
        "button.background": "#767b91",
        "button.hoverBackground": "#656a80",
        "textLink.foreground": "#88888f",
        "titleBar.activeBackground": "#1d1d1d",
        "titleBar.activeForeground": "#5f65b5",

        
        "terminal.foreground": "#5F65B5",
        "terminalCursor.foreground": "#5F65B5",
        "terminal.ansiBlue": "#5F65B5",
        "terminal.ansiBrightBlue": "#7176b5",

        
        "list.activeSelectionBackground": "#5F65B544",
        "list.inactiveSelectionBackground": "#5F65B522",
        "list.hoverBackground": "#1e1e1e",
        "sideBar.foreground": "#7176b5",

        
        "gitDecoration.modifiedResourceForeground": "#8E94D1",
        "gitDecoration.untrackedResourceForeground": "#7176B5",
        "gitDecoration.deletedResourceForeground": "#4A4F8F",
        "gitDecoration.ignoredResourceForeground": "#444763",
        "editor.selectionBackground": "#5F65B577",
        "editor.selectionHighlightBackground": "#5F65B544"
    },
    "tokenColors": [
    {
        "name": "Keywords",
        "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control.import.python"],
        "settings": { "foreground": "#9DA3FB", "fontStyle": "bold" } 
    },
    {
        "name": "Strings",
        "scope": ["string", "string.quoted", "constant.character.escape"],
        "settings": { "foreground": "#CE9178" }
    },
    {
        "name": "Variables",
        "scope": ["variable", "variable.other", "variable.parameter"],
        "settings": { "foreground": "#767B91" }
    },
    {
        "name": "FORCE RED: Methods and Classes",
        "scope": [
            "entity.name.function",
            "meta.function-call",
            "meta.function-call.python",
            "support.function",
            "entity.name.type.class",
            "support.class",
            "entity.name.function.member",
            "variable.function",
            "source.python meta.function-call.generic.python"
        ],
        "settings": { "foreground": "#9DA3FB", "fontStyle": "bold" }
    },
    {
        "name": "Comments",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": { "foreground": "#6A9955", "fontStyle": "italic" }
    },
    {
    "name": "Object Properties and Attributes",
    "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.magic.python",
        "meta.attribute.python",
        "entity.name.variable.property"
    ],
    "settings": {
        "foreground": "#b6c5fc" 
    }
}
]
}